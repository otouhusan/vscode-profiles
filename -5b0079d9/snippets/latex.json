{
    "sbi": {
        "prefix": "symbfit",
        "body": "\\symbfit{$1}"
    },
    "dagger": {
        "prefix": "\\dagger",
        "body": "^\\dagger"
    },
    "equation env": {
        "prefix": "equation",
        "body": [
            "%",
            "\\begin{equation}",
            "\t$1",
            "\\end{equation}",
            "%",
            "$0"
        ],
        "description": "simple equation"
    },
    "align env": {
        "prefix": ["align"],
        "body": [
            "%",
            "\\begin{align}",
            "\t$1",
            "\\end{align}",
            "%",
            "$0"
        ],
        "description": "align env"
    },
    "gather env": {
        "prefix": ["gather"],
        "body": [
            "%",
            "\\begin{gather}",
            "\t$1",
            "\\end{gather}",
            "%",
            "$0"
        ],
        "description": "gather env"
    },
    "equ ali env": {
        "prefix": ["ealign"],
        "body": [
            "%",
            "\\begin{equation}",
            "\t\\begin{aligned}[b]",
            "\t\t$1",
            "\t\\end{aligned}",
            "\\end{equation}",
            "%",
            "$0"
        ],
        "description": "aligned in equation"
    },
    "definition env": {
        "prefix": "dfn",
        "body": ["\\begin{dfn}",
            "\t$1",
            "\\end{dfn}"
        ],
        "description": ""
    },
    "adjustable()": {
        "prefix": "\\()",
        "body": "\\left( $1 \\right"
    },
    "adjustable{}": {
        "prefix": "\\{}",
        "body": "\\left\\{ $1 \\right\\"
    },
    "adjustable[]": {
        "prefix": "\\[]",
        "body": "\\left[ $1 \\right"
    },
    "adjustable||": {
        "prefix": "\\||",
        "body": "\\left| $1 \\right|"
    },
    "adjustable<>": {
        "prefix": "\\<>",
        "body": "\\left< $1 \\right>"
    },
    "textrm": {
        "prefix": "rm",
        "body": "\\textrm{$1}"
    },
    "coloneqq": {
        "prefix": "coloneqq",
        "body": "\\coloneqq ",
        "description": "with space"
    },
    "differential (1)": {
        "prefix": "dif1",
        "body": "\\frac{d$1}{d$2}",
        "description": "1st order differential"
    },
    "differential (2)": {
        "prefix": "dif2",
        "body": "\\frac{d$1^2}{d$2^2}",
        "description": "2nd order differential"
    },
    "<~~~>": {
        "prefix": "angle",
        "body": "\\langle $1\\rangle",
        "description": ""
    },
    "integer": {
        "prefix": "intger",
        "body": "\\mathbb{Z}",
        "description": ""
    },
    "real number field": {
        "prefix": "real",
        "body": "\\mathbb{R}",
        "description": ""
    },
    "complex number field": {
        "prefix": "comp",
        "body": "\\mathbb{C}",
        "description": ""
    },
    "colon equal": {
        "prefix": "coloneq",
        "body": "\\coloneqq",
        "description": ""
    },
    "inverse power": {
        "prefix": "^-1",
        "body": "^{-1}",
        "description": ""
    },
    "matrix": {
        "prefix": "matrix",
        "body": ["\\begin{pmatrix}",
                    "\t$1",
                "\\end{pmatrix}"],
        "description": ""
    },
    "infty integral": {
        "prefix": "intinf",
        "body": "\\int_{-\\infty}^\\infty",
        "description": ""
    },
    "xrightarrow": {
        "prefix": "\\xrightarrow",
        "body": "\\xrightarrow{$1}",
        "description": ""
    },
    "frac{1}{2}": {
        "prefix": "half",
        "body": "\\frac{1}{2}",
        "description": ""
    },
    "figure": {
        "prefix": "figure",
        "body": ["%",
            "\\begin{figure}",
            "\t\\centering",
            "\t% \\includegraphics[scale=0.5]{example-image}",
            "\t\\includegraphics[scale=1.0]{fig/$1.png}",
            "\t\\caption{}",
            "\t\\label{fig:}",
            "\\end{figure}",
            "%"
            ],
        "description": ""
    },
    "": {
        "prefix": "",
        "body": "",
        "description": ""
    }
}